	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}

 class data {
	
	private Object obj;

	public data(Object obj)	
	{
		 this.obj=obj;
		
	}
	@Override
	public String toString() {
		return "data [obj=" + obj + "]";
	}

	public Object getObj() {
		return obj;
	}
}
